    "    etc = input('입력할 메모 :')\n",
    "    newMember = Member(name, phone, email, age, grade, etc)\n",
    "    cursor.execute('INSERT INTO MEMBER VALUES (:name, :phone, :email, :age, :grade, :etc)', newMember.as_dic())\n",
    "    conn.commit()\n",
    "    cursor.close()\n",
    "    return newMember"
