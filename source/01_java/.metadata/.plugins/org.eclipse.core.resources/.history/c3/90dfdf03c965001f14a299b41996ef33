package com.lec.ex;

public class Ex02arratprint {
	public static void main(String[] args) {
		int [] arr = {0, 0, 0};
		// 확장 for을 이용한 출력 배열 출력을 할경우 사용
		for(int a : arr) {
			System.out.println(a);
		}
		// 일반 for문을 이용한 배열값 변경
		for(int a =0 ; a<arr.length ; a++) {
			arr[a] = 99;
			System.out.println("일반 for문을 이용한 배열값 변경");
		}
	}
}
