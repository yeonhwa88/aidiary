package com.lec.ex1_car;

public class CarMain {

	public static void main(String[] args) {
		Car mycar = new Car();
//		mycar.color = "아마존그레이";
		mycar.setColor("아마존그레이");
//		mycar.cc = 2400;
		mycar.setCc(120);
		System.out.println(mycar.Color + "색 차 배기량" + mycar.Cc + ",속도는"  + mycar.Speed);
		mycar.drive();
		mycar.park();
		mycar.race();
		System.out.println(mycar.color + "색 차 배기량" + mycar.cc + ",속도는"  + mycar.speed);
		Car yourcar = new Car();
//		yourcar.color = "검정";
		yourcar.setColor("검정");
		yourcar.drive();
		System.out.println(mycar.color + "색 차 배기량" + mycar.cc + ",속도는"  + mycar.speed);
		System.out.println(yourcar.color + "색 차 배기량" + yourcar.cc + ",속도는"  + yourcar.speed);
	}
	
}
