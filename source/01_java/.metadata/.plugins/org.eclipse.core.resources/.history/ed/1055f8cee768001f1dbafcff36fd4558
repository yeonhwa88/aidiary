//		int dansu = sc.nextInt();
		int dansu;
		System.out.println("입력한 단수는 :" + dansu);
		gugudanOut(dansu);
	}
	private static void gugudanOut(int dansu) {
		for ( int i=1 ; i<=9 ; i++) {
			for (int j=2 ; j<=9 ; j++) {
			}
			System.out.printf("%d x %d = %d\n", j, 1, j*i);
		}
		
	}
}
