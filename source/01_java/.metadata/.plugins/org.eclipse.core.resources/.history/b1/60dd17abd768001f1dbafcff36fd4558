package com.lec.ex;
// return 이 없는 메소드 타입은 void로 
public class Ex04_void {
	public static void main(String[] args) {
		printLine(30);
		System.out.println("Hello, World!");
		printLine();
		System.out.println("Hello, Java");
		printLine();
		System.out.println("Hello, World!");
		printLine();
	}
			public static void printLine() {
				for(int i=0 ; i<20 ; i++) {
					System.out.print("-");
				}
				System.out.println(); //개행
			}
			

}
