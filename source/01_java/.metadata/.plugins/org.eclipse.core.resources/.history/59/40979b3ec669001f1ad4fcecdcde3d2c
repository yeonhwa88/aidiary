package com.lec.ex3_static;

public class MomTestMain {
	public static void main(String[] args) {
		Child child1 = new Child("첫째 길동이");
		Child child2 = new Child("둘째 길동이");
		Child child3 = new Child("셋째 길동이");
		System.out.println("첫째 엄마지갑 : " + child1.mom.money);
		child1.takeMoney(1000);
		child1.takeMoney(1000);
		child1.takeMoney(1000);
		System.out.println("첫째 엄마지갑 : " + child1.mom.money);
	}
}
